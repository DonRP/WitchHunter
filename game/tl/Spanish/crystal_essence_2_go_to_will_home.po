# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:103 #-#-# translate crowdin crystal_essence_2_go_to_will_home_9f771de7:
msgid "[_will_] Оооох.. Р.. Рик.."
msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:104 #-#-# translate crowdin crystal_essence_2_go_to_will_home_0aa32ae5:
msgid "[_cedrick_] (Хах, я так и знал. Она уже не может себя контролировать)"
