msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:46 #-#-# translate crowdin crystal_essence_2_go_to_will_home_a9c1aa41:
msgid "[_cedrick_] Главное не забывай о блинчиках, скоро время завтрака"
msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:47 #-#-# translate crowdin crystal_essence_2_go_to_will_home_0d44cb56:
