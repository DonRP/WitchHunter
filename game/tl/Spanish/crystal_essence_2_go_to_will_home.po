msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:38 #-#-# translate crowdin crystal_essence_2_go_to_will_home_a7418d52:
msgid "[_cedrick_] (Ох, эта женщина будит во мне зверя)"
msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:39 #-#-# translate crowdin crystal_essence_2_go_to_will_home_24c3c3c0:
msgid "[_cedrick_] Ууух, Сьюзан, отлично сказано. Давай-ка проверим, насколько далеко ты готова зайти"
msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:44 #-#-# translate crowdin crystal_essence_2_go_to_will_home_df1ab82b:
msgid "[_cedrick_] Уффф, а как тебе такое, Сьюзан? Отличная поза для послушной домохозяйки, не считаешь?"
msgstr ""

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_go_to_will_home.rpy:45 #-#-# translate crowdin crystal_essence_2_go_to_will_home_4c95052f:
