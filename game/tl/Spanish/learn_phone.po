# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/learn_phone/learn_phone.rpy:7 #-#-# translate crowdin learn_phone_8e758cfd:
msgid "[_cedrick_] Так, а здесь у нас хранятся сообщения..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:8 #-#-# translate crowdin learn_phone_7302d63e:
msgid "[_cedrick_] Возможно, здесь я смогу узнать о предыдущем владельце телефона..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:17 #-#-# translate crowdin learn_phone_bda3f0d2:
msgid "[_cedrick_] Хм... Похоже она была любящей женой..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:22 #-#-# translate crowdin learn_phone_2dd6fbad:
msgid "[_cedrick_] Или проституткой, хе-хе"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:27 #-#-# translate crowdin learn_phone_37db2bb0:
msgid "[_cedrick_] Которая изменяла своему мужу с любовником..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:33 #-#-# translate crowdin learn_phone_198a54d7:
msgid "[_cedrick_] Что ж, это все очень трогательные переписки, но думаю, что это мне не пригодится..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:35 #-#-# translate crowdin learn_phone_039c86e3:
msgid "[_cedrick_] Так, глянем что у нас дальше..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:45 #-#-# translate crowdin learn_phone_yea_ff909fa1:
msgid "[_Somka108_] Привет! Пока Седрик с умным лицом тыкается в телефоне, я вкратце объясню тебе как устроен его интерфейс"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:47 #-#-# translate crowdin learn_phone_yea_133c3678:
msgid "[_Somka108_] Это меню сообщений"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:49 #-#-# translate crowdin learn_phone_yea_2cedfe31:
msgid "[_Somka108_] В меню сообщения, иногда по сюжету можно будет отправить сообщение кому-нибудь, кто уже есть у вас в контактах."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:51 #-#-# translate crowdin learn_phone_yea_3dff3472:
msgid "[_Somka108_] Далее идет меню контакты"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:53 #-#-# translate crowdin learn_phone_yea_237632b0:
msgid "[_Somka108_] Здесь вы сможете позвонить кому-нибудь, что можно сделать довольно редко, в принципе как и с отправкой сообщений)"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:55 #-#-# translate crowdin learn_phone_yea_7e508f98:
msgid "[_Somka108_] Потом идет журнал квестов."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:57 #-#-# translate crowdin learn_phone_yea_394eb27f:
msgid "[_Somka108_] Здесь вы можете посмотреть текущие и доступные квесты."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:59 #-#-# translate crowdin learn_phone_yea_e71ca859:
msgid "[_Somka108_] Если вы вдруг забыли или не понимаете что нужно сделать, можете нажать на квест и посмотреть текущую задачу"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:61 #-#-# translate crowdin learn_phone_yea_482c820a:
msgid "[_Somka108_] Также вы можете посмотреть уже выполненные квесты и задания."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:62 #-#-# translate crowdin learn_phone_yea_b7aca2f4:
msgid "[_Somka108_] В игре восклицательные знаки указывают на места и предметы которые могут начать квест или нужны для прохождения текущего квеста"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:63 #-#-# translate crowdin learn_phone_yea_4367c50f:
msgid "[_Somka108_] В принципе, если у вас не начинается какой либо из квестов, даже если он отображается в вашем телефоне, просто начните проходить другие. Рано или поздно у вас начнется тот квест, который не удавалось начать ранее. Возможно, что вам будет необходимо побывать в какой-то локации в определенное время суток или пройти сюжетную линию с другим персонажем."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:65 #-#-# translate crowdin learn_phone_yea_e3e7c1c4:
msgid "[_Somka108_] Далее идет меню характеристик"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:67 #-#-# translate crowdin learn_phone_yea_61bb2cd1:
msgid "[_Somka108_] Здесь отображаются статы седрика. Тут все просто!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:69 #-#-# translate crowdin learn_phone_yea_b1f10418:
msgid "[_Somka108_] Следующее меню — Интернет Браузер"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:71 #-#-# translate crowdin learn_phone_yea_67094ada:
msgid "[_Somka108_] Тут периодически будут появляться различные действия для продвижения сюжета. (Просто заглядывайте туда время от времени, если видите восклицательный знак над ним...)"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:73 #-#-# translate crowdin learn_phone_yea_4940b0f1:
msgid "[_Somka108_] Сверху над главным меню вы можете увидеть день недели (иногда день недели влияет на определенные события)"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:75 #-#-# translate crowdin learn_phone_yea_eec9cd24:
msgid "[_Somka108_] Ну и оставшиеся кнопки... Снизу кнопка домой, она возвращает вас в главное меню."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:77 #-#-# translate crowdin learn_phone_yea_d110d97a:
msgid "[_Somka108_] Кнопка назад перенесет вас в предыдущее меню."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:79 #-#-# translate crowdin learn_phone_yea_bb7fc38e:
msgid "[_Somka108_] Кнопка Выключить — убирает телефон."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:80 #-#-# translate crowdin learn_phone_yea_2d3ccc20:
msgid "[_Somka108_] Фуф, ну вроде все! От всей команды разработчиков желаем вам хорошей игры!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:86 #-#-# translate crowdin learn_phone_next_b8700daa:
msgid "[_elion_] Оу, я вижу ты наконец-таки приобрел телефон!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:89 #-#-# translate crowdin learn_phone_next_6048632d:
msgid "[_cedrick_] Да, вроде того. Просто пытаюсь угнаться за современным миром. Прикольная мобилка, да?"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:92 #-#-# translate crowdin learn_phone_next_96911177:
msgid "[_elion_] Это похвально, Рик! ты молодец!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:94 #-#-# translate crowdin learn_phone_next_97b29729:
msgid "[_elion_] Кстати, ты мог бы прорекламировать свой магазин в интернете..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:97 #-#-# translate crowdin learn_phone_next_582993d0:
msgid "[_cedrick_] Это как?"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:100 #-#-# translate crowdin learn_phone_next_0cf68c20:
msgid "[_elion_] Ну, скажем, можно подать рекламное объявление в Хезерфилд Ньюс о том, что собираешься устроить горящую распродажу..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:101 #-#-# translate crowdin learn_phone_next_4f2d9696:
msgid "[_cedrick_] Я не уверен, что смогу позволить себе делать большие скидки на эти книжки..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:104 #-#-# translate crowdin learn_phone_next_a9b99fb0:
msgid "[_elion_]  Ну... Тогда можно устроить какое-нибудь мероприятие... Где читатели будут делиться отрывками из своих любимых книг..."
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:107 #-#-# translate crowdin learn_phone_next_b8e83a72:
msgid "[_cedrick_] Что-то вроде книжного клуба?"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:110 #-#-# translate crowdin learn_phone_next_ef629f1f:
msgid "[_elion_] Даки-даки! Вроде того!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:113 #-#-# translate crowdin learn_phone_next_6e87c434:
msgid "[_cedrick_] И люди смогут обсуждать свои любимые книги вживую. Это привлечет внимание к моему магазину!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:115 #-#-# translate crowdin learn_phone_next_b4e23a31:
msgid "[_cedrick_] Отличная идея, Элион, спасибо!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:119 #-#-# translate crowdin learn_phone_next_2ba417d7:
msgid "[_elion_] Хах! не за что, Рик... обращайся!"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:123 #-#-# translate crowdin learn_phone_next_b9fc54f8:
msgid "Теперь в браузере вашего телефона появилась новая опция — Устроить публичные чтения — С ее помощью вы можете повысить навык красноречия у Седрика, который будет необходим для получения некоторых сцен или более легкого прохождения игры в некоторых ситуациях развилках..."
msgstr ""

# §translate crowdin strings: #|#|# # game/scripts/quests/learn_phone/learn_phone.rpy:37
msgid "Вы хотите пройти краткий обзор по функционалу телефона?"
msgstr ""

# game/scripts/quests/learn_phone/learn_phone.rpy:37
msgid "Нет(пропустить)"
msgstr ""

