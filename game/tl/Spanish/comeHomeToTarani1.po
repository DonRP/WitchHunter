# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/tarani/comeHomeToTarani1.rpy:7 #-#-# translate crowdin comeHomeToTarani1_b4fbddde:
msgid "[_cedrick_] Ну, если сестры Грампер дали мне верный адрес, то это должно быть дом Тарани..."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:10 #-#-# translate crowdin comeHomeToTarani1_647a937c:
msgid "[_cedrick_] Кажется, внутри дома кто-то ругается.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:11 #-#-# translate crowdin comeHomeToTarani1_961d6315:
msgid "[_womanVoice_] Ты никуда не пойдешь В ТАКОМ ВИДЕ!!!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:12 #-#-# translate crowdin comeHomeToTarani1_141640e4:
msgid "[_tarani_] Отстань от меня! Я уже взрослая!!! И сама буду решать, что мне делать!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:13 #-#-# translate crowdin comeHomeToTarani1_f4a0bfeb:
msgid "[_woman_] Я твоя мать и пока ты живешь в этом доме — ты будешь делать то, что я тебе говорю!!!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:14 #-#-# translate crowdin comeHomeToTarani1_e6fc69c6:
msgid "[_tarani_]  Ну же! ПОПРОБУЙ ОСТАНОВИТЬ МЕНЯ!!! КАК МНЕ ВСЕ ЭТО НАДОЕЛО!!!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:17 #-#-# translate crowdin comeHomeToTarani1_a20cefa7:
msgid "..."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:21 #-#-# translate crowdin comeHomeToTarani1_21c79941:
msgid "Тарани выбегает из дома и хлопает дверью."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:25 #-#-# translate crowdin comeHomeToTarani1_87a437e6:
msgid "[_cedrick_] Надо же, как резво побежала. Хм, может проследовать за ней?"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:28 #-#-# translate crowdin comeHomeToTarani1_87882ed9:
msgid "[_woman_] Тарани, постой!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:32 #-#-# translate crowdin comeHomeToTarani1_a583bacf:
msgid "[_cedrick_] (Вот черт, эта женщина меня заметила. Теперь будет странно, если я побегу за Тарани.. Но, возможно, я смогу извлечь из этого выгоду)"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:33 #-#-# translate crowdin comeHomeToTarani1_0be83ed7:
msgid "[_cedrick_] Здравствуйте, я Рик Хоффман, школьный учитель истории у Тарани. Я так понимаю, она только что убежала.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:37 #-#-# translate crowdin comeHomeToTarani1_9e74711a:
msgid "[_woman_] Да, убежала на вечеринку к этому своему паршивцу, Найджелу!.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:41 #-#-# translate crowdin comeHomeToTarani1_43c73c52:
msgid "[_woman_] Ох, простите за грубость. Здравствуйте, меня зовут Тереза Кук, я мама Тарани.. А вы, собственно, по какому делу решили к нам зайти?"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:45 #-#-# translate crowdin comeHomeToTarani1_e41eae92:
msgid "[_cedrick_] Опять этот Найджел.. Я как раз хотел с вами о нем поговорить. Я обеспокоен тем, как Найджел влияет на вашу Тарани"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:49 #-#-# translate crowdin comeHomeToTarani1_26d81873:
msgid "[_theresa_] Думаете я не знаю.. Она по уши влюблена в этого мальчишку, а он то и дело склоняет ее на кривую дорожку. Видели, как она была одета?!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:53 #-#-# translate crowdin comeHomeToTarani1_76843062:
msgid "[_cedrick_] Да, это.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:54 #-#-# translate crowdin comeHomeToTarani1_2b3e2e3e:
msgid "[_cedrick_] (Это было чертовски горячо. Надеюсь, успею трахнуть ее в этом костюме)"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:55 #-#-# translate crowdin comeHomeToTarani1_2b2925d9:
msgid "[_cedrick_] Это очень возмутительно.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:59 #-#-# translate crowdin comeHomeToTarani1_3c4e8ffd:
msgid "[_theresa_] Именно!.. Вы не представляете, как я за нее переживаю.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:63 #-#-# translate crowdin comeHomeToTarani1_3d1f554a:
msgid "[_theresa_] Еще и ходят слухи, что у нас в городе завелся маньяк и уже несколько людей пропало.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:64 #-#-# translate crowdin comeHomeToTarani1_5728c53a:
msgid "[_cedrick_] Надеюсь, этот Найджел сможет ее защитить.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:68 #-#-# translate crowdin comeHomeToTarani1_f6be80a3:
msgid "[_theresa_] Пх.. Этот Найджел и его компания.. Я работаю судьей и пару лет назад проходил суд над его братом! Его посадили в тюрьму на три с половиной года, он ограбил магазин игрушек под рождество!"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:69 #-#-# translate crowdin comeHomeToTarani1_0800873c:
msgid "[_theresa_] Этот малолетний преступник и его брат Найджел. Сомневаюсь, что они вообще будут кого-то защищать"
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:73 #-#-# translate crowdin comeHomeToTarani1_72bc2dac:
msgid "[_theresa_] На днях его брата освободили из тюрьмы досрочно и я боюсь, что ситуация может усугубиться.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:74 #-#-# translate crowdin comeHomeToTarani1_2ceab40f:
msgid "[_cedrick_] Я бы очень хотел вам помочь, но не знаю как.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:78 #-#-# translate crowdin comeHomeToTarani1_f246a486:
msgid "[_theresa_] Ладно.. Простите, что вывалила на вас все это. Спасибо, что беспокоитесь о моей дочери, но я и сама уже не знаю, что с ней делать. Все этот Найджел.."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:80 #-#-# translate crowdin comeHomeToTarani1_5f9b772e:
msgid "...."
msgstr ""

# game/scripts/quests/tarani/comeHomeToTarani1.rpy:81 #-#-# translate crowdin comeHomeToTarani1_266846d5:
msgid "[_cedrick_] (Так значит, у Найджела есть брат преступник, который недавно вышел из тюрьмы. Пожалуй, стоит снова попросить Бланка проследить за ним, возможно он сможет заснять более ценные кадры...)"
msgstr ""

