# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian4.rpy:60 #-#-# translate crowdin comeToLilianInMeridian4_1808b8b9:
msgid "[_cedrick_] Вот так, хорошая девочка..."
msgstr "[_cedrick_] Voilà, bonne fille..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian4.rpy:61 #-#-# translate crowdin comeToLilianInMeridian4_a20cefa7_5:
