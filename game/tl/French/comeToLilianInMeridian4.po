# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian4.rpy:50 #-#-# translate crowdin comeToLilianInMeridian4_8f57d57b:
msgid "[_cedrick_] Все хорошо? Я продолжу?"
msgstr "[_cedrick_] Tout va bien ? Dois-je continuer ?"

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian4.rpy:51 #-#-# translate crowdin comeToLilianInMeridian4_8aa38c5d:
msgid "[_lilian_] Да..."
msgstr "[_lilian_] Oui..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian4.rpy:52 #-#-# translate crowdin comeToLilianInMeridian4_a20cefa7_2:
