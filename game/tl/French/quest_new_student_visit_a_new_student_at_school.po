msgid "[_cedrick_] (Что-то она там долго...)"
msgstr "[_cedrick_] (Il y a quelque chose qui lui prend beaucoup de temps là...)"

# game/scripts/quests/newStudent/quest_new_student_visit_a_new_student_at_school.rpy:82 #-#-# translate crowdin quest_new_student_visit_a_new_student_at_school_23137bfe:
