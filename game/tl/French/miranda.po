msgstr "Enlever l'essence cristalline"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Приготовить зелья"
msgstr "Préparer les potions"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Купить зелье"
msgstr "Acheter une potion"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Продать ингредиенты"
msgstr "Vendre des ingrédients"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Где мне достать золото?"
msgstr "Où puis-je trouver de l'or ?"

