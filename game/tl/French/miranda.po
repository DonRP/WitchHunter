msgstr "Une potion pour Yang Lin"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Попросить клонировать Тома Леира"
msgstr "Demander à Tom Leir d'être cloné"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Отдать клок волос для приготовления зелья"
msgstr "Donner une mèche de cheveux pour faire une potion"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Попросить зелье возраста у миранды"
msgstr "Demandez à Miranda une potion de vieillissement"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Попросить ноздрю дракона"
msgstr "Demandez la narine du dragon"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Узнать о девушке в колбе"
msgstr "Découvrir la fille dans la flasque"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "10 Желез морпионов"
msgstr "10 Fer du Morpion"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Поиск королевы морпионов"
msgstr "Recherche de la reine des morpions"

# game/scripts/meridian/miranda/miranda.rpy:12
msgid "Я добыл Кристаллическую эссенцию"
