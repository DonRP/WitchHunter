# game/scripts/quests/willCrystalEssence2/crystal_essence_2_fight_escape_from_heaterfield.rpy:6 #-#-# translate crowdin crystal_essence_2_fight_escape_from_heaterfield_d3e729b6:
msgid "[_irma_] Не суйся под руку, шестерка Фобоса!"
msgstr "[_irma_] Restez hors de mon chemin, les six de Phobos !"

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_fight_escape_from_heaterfield.rpy:10 #-#-# translate crowdin crystal_essence_2_fight_escape_from_heaterfield_763a37a1:
msgid "[_cedrick_] Миранда!!"
msgstr "[_cedrick_] Miranda ! !!"

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_fight_escape_from_heaterfield.rpy:11 #-#-# translate crowdin crystal_essence_2_fight_escape_from_heaterfield_81d6b723:
msgid "[_miranda_] Р.. Рик.."
msgstr "[_miranda_] Р.. Rick..."

# game/scripts/quests/willCrystalEssence2/crystal_essence_2_fight_escape_from_heaterfield.rpy:12 #-#-# translate crowdin crystal_essence_2_fight_escape_from_heaterfield_87b7ed4a:
