msgid "[_cedrick_] Я принес тебе золото!"
msgstr "[_cedrick_] Je vous ai apporté de l'or !"

# game/scripts/meridian/tavern/labels/herald.rpy:22 #-#-# translate crowdin herald_b538f059:
msgid "[_herald_] Тогда давай-же сыграем!"
msgstr "[_herald_] Alors, jouons !"

# game/scripts/meridian/tavern/labels/herald.rpy:32 #-#-# translate crowdin herald_d1141bd0:
msgid "[_herald_] Ты принес золото?"
msgstr "[_herald_] Tu as apporté l'or ?"

# game/scripts/meridian/tavern/labels/herald.rpy:33 #-#-# translate crowdin herald_cbcc2ae8:
msgid "[_cedrick_] Нет... Еще нет..."
msgstr "[_cedrick_] Non... Pas encore..."

# game/scripts/meridian/tavern/labels/herald.rpy:34 #-#-# translate crowdin herald_6f71cd4c:
msgid "[_herald_] Тогда не мешай мне запивать свое горе!"
msgstr "[_herald_] Alors ne m'empêchez pas de boire mon chagrin !"

# game/scripts/meridian/tavern/labels/herald.rpy:37 #-#-# translate crowdin herald_1a1d29d6:
msgid "[_cedrick_] Слушай давай сыграем на твою доску"
msgstr "[_cedrick_] Ecoutez, jouons votre tableau"

# game/scripts/meridian/tavern/labels/herald.rpy:38 #-#-# translate crowdin herald_880c6091:
msgid "[_herald_] С тебя тогда 200 золотых"
msgstr "[_herald_] Ça fera 200 pièces d'or."

# game/scripts/meridian/tavern/labels/herald.rpy:53 #-#-# translate crowdin herald_91805c1d:
msgid "[_herald_] Ну тогда в другой раз"
msgstr "[_herald_] Une autre fois, alors."

