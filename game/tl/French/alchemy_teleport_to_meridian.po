# game/scripts/quests/joiningsAfterMissingBryan/alchemy_teleport_to_meridian.rpy:32 #-#-# translate crowdin alchemy_teleport_to_meridian_9aabe7ea:
msgid "[_cedrick_sprite_] Что ты здесь делаешь?"
msgstr "[_cedrick_sprite_] Que faites-vous ici ?"

# game/scripts/quests/joiningsAfterMissingBryan/alchemy_teleport_to_meridian.rpy:33 #-#-# translate crowdin alchemy_teleport_to_meridian_ee05a806:
