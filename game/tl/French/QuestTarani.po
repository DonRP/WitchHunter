# TODO: Translation updated at 2022-12-22 01:40 #|#|# # §translate crowdin strings: #|#|# # game/scripts/core/quests/QuestTarani.rpy:3
msgid "Тарани Кук"
msgstr "Tarani Cook"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Провести первый урок"
msgstr "Organiser la première leçon"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Провести урок и проверить контрольную работу занизив оценку Тарани"
msgstr "Diriger la leçon et vérifier le travail du test en diminuant la note de Tarani."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Раздать контрольные работы на следующем уроке"
msgstr "Distribuez les tests de la prochaine leçon"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Занизить оценку домашней работы Тарани"
msgstr "Baisser la note des devoirs de Tarani"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Поговорить с Тарани после урока"
msgstr "Parlez à Tarani après le cours"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Провести очередной урок"
msgstr "Organiser une autre leçon"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Распросить о Тарани в школьном клубе журналистике"
msgstr "Posez des questions sur Tarani au club de journalisme de l'école."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Достать микрофон для клуба журналистики"
msgstr "Obtenir un microphone pour le club de journalisme"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Отнести микрофон Бэсс и Кортни и разузнать о Тарани"
msgstr "Prends le micro pour Bess et Courtney et renseigne-toi sur Tarani."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Бланка сделать фотографии Найджела"
msgstr "Demandez à Blank de prendre des photos de Nigel"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Достать фотоаппарат для Бланка"
msgstr "Acheter un appareil photo pour Blank"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Узнать у миранды где достать Ноздрю дракона"
msgstr "Demande à Miranda où trouver la narine du dragon."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Добыть клык Ларвека Миранде в обмен на Ноздрю дракона"
msgstr "Obtenir un croc de Larvek pour Miranda en échange de la narine du dragon."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Отдать Бланку Ноздрю дракона и фотоаппарат"
msgstr "Donnez à Blanca la narine du dragon et l'appareil photo."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Дождаться когда Бланк сделает фотографии Найджела и забрать их"
msgstr "Attendez que Blank prenne les photos de Nigel et récupérez-les."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Шантажировать Тарани фотографиями Найджела"
msgstr "Faire chanter Tarani avec des photos de Nigel"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Тарани сделать нам приятное после урока"
msgstr "Demander à Tarani de nous faire sentir bien après le cours."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Тарани поработать ртом"
msgstr "Demander à Tarani de travailler avec sa bouche"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Навестить Тарани у нее дома"
msgstr "Visite de Tarani à son domicile"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Бланка сделать новые фотографии Найджела"
msgstr "Demandez à Blank de prendre de nouvelles photos de Nigel."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Добыть Бланку золотую цепочку либо найти ему девушку для секса"
msgstr "Offrez à Blanca une chaîne en or ou trouvez-lui une fille avec qui faire l'amour."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Навестить Тарани у нее дома вечером и показать ей фотографии Найджела"
msgstr "Rendez visite à Tarani chez elle ce soir et montrez-lui des photos de Nigel."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Поговорить с Тарани в школе"
msgstr "Parlez à Tarani à l'école"

