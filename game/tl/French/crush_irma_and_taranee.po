# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:8 #-#-# translate crowdin crush_irma_and_taranee_96c6aca9:
msgid "[_tarani_] Н-нет, стой.. Как же.. Я не могла проиграть.. Ч-черт, ноги не слушаются.. Я слишком слаба.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:9 #-#-# translate crowdin crush_irma_and_taranee_d34a09bc:
msgid "[_cedrick_] И все же проиграла. Ты держалась достойно, стоит признать. Из всех стражниц только твой дух я все еще но смог сломить"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:10 #-#-# translate crowdin crush_irma_and_taranee_533082e8:
msgid "[_tarani_] Т-только мой? Ты все еще самоуверен.. Есть еще Ирма.. И Вилл.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:11 #-#-# translate crowdin crush_irma_and_taranee_92edc05d:
msgid "[_cedrick_] Хах, Вилл? Ты явно всего не знаешь. Что же касается Ирмы, то давай посмотрим как она там"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:15 #-#-# translate crowdin crush_irma_and_taranee_a432e95d:
msgid "[_irma_] Ах.. Ах.. Ч-чертов тарантул.. Моя вода не действует против паутины.. Не честно.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:16 #-#-# translate crowdin crush_irma_and_taranee_2cfad58f:
msgid "[_cedrick_] Что ж, думаю пока Тарани там отдыхает, самое время наконец заняться тобой"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:18 #-#-# translate crowdin crush_irma_and_taranee_c8d566e6:
msgid "[_irma_] Ч-черт, нет, только не снова.. Не сейчас.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:19 #-#-# translate crowdin crush_irma_and_taranee_26ad33f0:
msgid "[_cedrick_] Миранда, поможешь мне?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:21 #-#-# translate crowdin crush_irma_and_taranee_1c3e8727:
msgid "[_miranda_] Без проблем. Свяжу ее и оставлю вас наедине. Присмотрю пока, чтобы Тарани тебе не мешала"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:22 #-#-# translate crowdin crush_irma_and_taranee_3d4d3f2a:
msgid "[_cedrick_] Отличный план"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:24 #-#-# translate crowdin crush_irma_and_taranee_3617bd3b:
msgid "[_irma_] С-стойте.. Хватит.. Я уже не могу.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:28 #-#-# translate crowdin crush_irma_and_taranee_d46c57b8:
msgid "[_irma_] Стой.. Рик, прошу, не нужно.. Если хочешь, я пообещаю, что больше никогда не нападу на вас с Мирандой.. Дай мне просто уйти.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:29 #-#-# translate crowdin crush_irma_and_taranee_ccd0d0e1:
msgid "[_cedrick_] Ох ты ж.. Не ожидал от тебя такого. А Тарани еще считала, что я тебя еще не сломил.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:31 #-#-# translate crowdin crush_irma_and_taranee_795678bb:
msgid "[_irma_] ..."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:32 #-#-# translate crowdin crush_irma_and_taranee_bd720cb0:
msgid "[_cedrick_] Бедная девочка, смотрю ты совсем уже выбилась из сил.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:34 #-#-# translate crowdin crush_irma_and_taranee_87144e8c:
msgid "[_irma_] Я.. Я не такой боец, как Тарани.. Я очень хотела быть такой.. Но я так устала.. Устала сражаться.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:36 #-#-# translate crowdin crush_irma_and_taranee_4d2873fe:
msgid "[_cedrick_] (Ооо, все даже хуже, чем я думал.. смотрю ее совсем накрыло. Кажется, в психологии это называется отложенной депрессией)"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:37 #-#-# translate crowdin crush_irma_and_taranee_47dfdada:
msgid "[_cedrick_] (Видимо она уже давно была на пределе и у нее оставались силы сражаться вплоть до первого проигрыша)"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:38 #-#-# translate crowdin crush_irma_and_taranee_c0f419b5:
msgid "[_cedrick_] (Как боксер на ринге, который все еще стоит на ногах и машинально машет руками, но которому до отключки остался один небольшой толчок)"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:40 #-#-# translate crowdin crush_irma_and_taranee_afb684c8:
msgid "[_irma_] Тарани движет месть за своего парня, за свое унижения, она готова рвать и метать.. А я, а что я.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:41 #-#-# translate crowdin crush_irma_and_taranee_70c54cac:
msgid "[_irma_] Я ничего этого уже не хочу.. Ты меня шантажировал, унижал, манипулировал мной.. А мне уже плевать.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:42 #-#-# translate crowdin crush_irma_and_taranee_cc5f69bc:
msgid "[_irma_] Я просто хочу перестать жить постоянной борьбой с чем-то.. Если для этого придется позволить тебе меня трахнуть - так тому и быть.. Только не заставляй меня снова сражаться.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:44 #-#-# translate crowdin crush_irma_and_taranee_da6a45bb:
msgid "[_cedrick_] Ох, я понимаю, Ирма.. Ты на пределе, я совсем недавно был в твоем положении"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:49 #-#-# translate crowdin crush_irma_and_taranee_795678bb_1:
msgid "[_irma_] ..."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:51 #-#-# translate crowdin crush_irma_and_taranee_757e7ee2:
msgid "[_cedrick_] Но знаешь, в такой ситуации есть два выхода. Ты либо сражаешься до последнего, готовый умереть, но не смириться с судьбой.. Либо ты принимаешь ситуацию"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:53 #-#-# translate crowdin crush_irma_and_taranee_6b47be27:
msgid "[_cedrick_] И что же сделаешь ты?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:55 #-#-# translate crowdin crush_irma_and_taranee_67eee041:
msgid "[_irma_] Я принимаю! Принимаю.. Если принять твой член означает закончить все это, если после этого мне дадут просто уйти, я готова.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:57 #-#-# translate crowdin crush_irma_and_taranee_29bc1983:
msgid "[_cedrick_] Хах.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:60 #-#-# translate crowdin crush_irma_and_taranee_6d7789d7:
msgid "[_irma_] Аааах.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:62 #-#-# translate crowdin crush_irma_and_taranee_3f9ad437:
msgid "[_cedrick_] Но, знаешь что, Ирма.. Я не говорил, что отпущу тебя, если ты примешь мой член"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:64 #-#-# translate crowdin crush_irma_and_taranee_dba70129:
msgid "[_irma_] Ч-что?.. Н-нет же, подожди.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:66 #-#-# translate crowdin crush_irma_and_taranee_6f3c67b7:
msgid "[_cedrick_] Смотри как получается.. Ты готова сдаться, но.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:68 #-#-# translate crowdin crush_irma_and_taranee_4d4ba5c2:
msgid "[_irma_] Аааххх.. Н-но?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:70 #-#-# translate crowdin crush_irma_and_taranee_4b5bfc22:
msgid "[_cedrick_] Но ты все еще стражница.. Ты все еще угроза, ты все еще мой враг.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:72 #-#-# translate crowdin crush_irma_and_taranee_9e922c45:
msgid "[_irma_] Н-нет, я же.. Я больше не буду... ааах.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:74 #-#-# translate crowdin crush_irma_and_taranee_6b32b4d3:
msgid "[_cedrick_] Ууух.. И, помимо всего прочего.. Ты навредила Миранде.. Ты попыталась убить единственного человека, который действительно для меня важен, понимаешь?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:76 #-#-# translate crowdin crush_irma_and_taranee_cc2a0ebb:
msgid "[_irma_] П-прости.. Прости меня, Рик.. Я больше никогда не перейду тебе дорогу.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:78 #-#-# translate crowdin crush_irma_and_taranee_3caa8409:
msgid "[_cedrick_] Ох, точно не перейдешь? А знаешь почему? Потому что у тебя не хватит на это сил"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:79 #-#-# translate crowdin crush_irma_and_taranee_795678bb_2:
msgid "[_irma_] ..."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:80 #-#-# translate crowdin crush_irma_and_taranee_84afacad:
msgid "[_cedrick_] Как и у любой из вас. Какую бы силу не дало вам сердце кондракара, воина определяет не сила, а его дух, его готовность сражаться"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:81 #-#-# translate crowdin crush_irma_and_taranee_c95875dc:
msgid "[_cedrick_] У вас же.. У вас дух самых обычных девчонок.. Вы не воины и вы не созданы, чтобы сражаться. Ты не создана"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:83 #-#-# translate crowdin crush_irma_and_taranee_d7f4a0fc:
msgid "[_irma_] Аааав.. Я-я знаю.. Я не боец.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:85 #-#-# translate crowdin crush_irma_and_taranee_c0a06cb5:
msgid "[_cedrick_] Ты не боец, но ты все еще опасна, и ты все еще наш враг. Знаешь, что делают со слабыми врагами, готовыми сдаться?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:87 #-#-# translate crowdin crush_irma_and_taranee_7d39e348:
msgid "[_irma_] Н-не надо.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:89 #-#-# translate crowdin crush_irma_and_taranee_e4833161:
msgid "[_cedrick_] Хах, ты понимаешь.. Твои бедра начинают сильнее дрожать.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:91 #-#-# translate crowdin crush_irma_and_taranee_98097936:
msgid "[_cedrick_] А теперь, Ирма.. Тебе пора усвоить кое что раз и навсегда. Одну простую мысль. Ты готова?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:93 #-#-# translate crowdin crush_irma_and_taranee_d6ac1e91:
msgid "[_irma_] Д-да.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:95 #-#-# translate crowdin crush_irma_and_taranee_1793af14:
msgid "[_cedrick_] Твой единственный выход, при котором ты остаешься в живых. Единственный выход, при котором ты не будешь вынуждена вечно сражаться и в страхе оглядываться по сторонам"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:98 #-#-# translate crowdin crush_irma_and_taranee_a751618d:
msgid "[_irma_] Е-единственный выход.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:100 #-#-# translate crowdin crush_irma_and_taranee_c1809038:
msgid "[_cedrick_] Это присоединиться ко мне, Ирма. Ты перестанешь быть моим врагом, я перестану желать тебе мести и тебе больше ничего не будет угрожать.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:101 #-#-# translate crowdin crush_irma_and_taranee_e157def8:
msgid "[_cedrick_] Только если ты больше никогда не будешь мне перечить"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:102 #-#-# translate crowdin crush_irma_and_taranee_795678bb_3:
msgid "[_irma_] ..."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:103 #-#-# translate crowdin crush_irma_and_taranee_bcfdb974:
msgid "[_cedrick_] А иначе.. Иначе на тебя обрушится вся сила и весь ужас Меридиана. Ты поняла?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:105 #-#-# translate crowdin crush_irma_and_taranee_4f59974d:
msgid "[_irma_] Ааах.. Д-да.. Я поняла.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:107 #-#-# translate crowdin crush_irma_and_taranee_4c974d93:
msgid "[_cedrick_] Повтори, что ты поняла! Что ты решила, девчонка!?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:109 #-#-# translate crowdin crush_irma_and_taranee_6d64e024:
msgid "[_irma_] Я.. Я больше никогда не буду тебе перечить.. Никогда.. Если так я буду в безопасности.. Если так я перестану жить постоянной борьбой.. Я согласна.. Это.. Это.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:111 #-#-# translate crowdin crush_irma_and_taranee_63a46d7b:
msgid "[_cedrick_] Это твой единственный выход, Ирма"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:113 #-#-# translate crowdin crush_irma_and_taranee_ebdceab5:
msgid "[_irma_] Ммммв.. Д-да.. Мой единственный... Ааах... Я, я кончаю..."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:115 #-#-# translate crowdin crush_irma_and_taranee_575a58d5:
msgid "[_cedrick_] И я.. Получай, Ирма. Навсегда запомни этот момент!"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:117 #-#-# translate crowdin crush_irma_and_taranee_80a5e90d:
msgid "[_irma_] Аааааааххх..."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:118 #-#-# translate crowdin crush_irma_and_taranee_5e63c793:
msgid "[_cedrick_] Ч-черт.. Нужны зелья.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:121 #-#-# translate crowdin crush_irma_and_taranee_42c7c647:
msgid "[_centered_] {size=37}Рик принимает 5 зелий восстановления магической силы{/size}"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:125 #-#-# translate crowdin crush_irma_and_taranee_99cf4cf0:
msgid "[_cedrick_] Миранда, не поможешь избавиться от паутины?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:126 #-#-# translate crowdin crush_irma_and_taranee_54267892:
msgid "[_miranda_] Конечно. Вы закончили?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:127 #-#-# translate crowdin crush_irma_and_taranee_f56c10f1:
msgid "[_cedrick_] Да, она готова. Как там Тарани?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:128 #-#-# translate crowdin crush_irma_and_taranee_5c324d0f:
msgid "[_miranda_] Пока в отключке"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:129 #-#-# translate crowdin crush_irma_and_taranee_fe662f75:
msgid "[_cedrick_] Ирма тоже.. Хах, подождем ка, пока они обе очнутся"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:130 #-#-# translate crowdin crush_irma_and_taranee_23654efb:
msgid "[_miranda_] Ох, Седрик, что ты задумал?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:131 #-#-# translate crowdin crush_irma_and_taranee_b54a7a27:
msgid "[_cedrick_] Хах, скоро увидишь"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:132 #-#-# translate crowdin crush_irma_and_taranee_6a1543f7:
msgid "Тарани просыпается.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:133 #-#-# translate crowdin crush_irma_and_taranee_3bc36509:
msgid "[_tarani_] Мхх.. Ч-что.. Моя голова.. Что случилось.."
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:137 #-#-# translate crowdin crush_irma_and_taranee_7cfb00e1:
msgid "[_tarani_] Ах!? Ч-что!?"
msgstr ""

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:138 #-#-# translate crowdin crush_irma_and_taranee_89e0bc95:
msgid "[_irma_] Прости, Тарани.."
msgstr ""

