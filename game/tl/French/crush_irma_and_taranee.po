# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:357 #-#-# translate crowdin crush_irma_and_taranee_f55ef7c2:
msgid "[_cedrick_] Так, а ну прекратили этот балаган!"
msgstr "[_cedrick_] Très bien, arrêtez tout ce brouhaha !"

# game/scripts/quests/crushingOfIrmaAndTaranee/crush_irma_and_taranee.rpy:359 #-#-# translate crowdin crush_irma_and_taranee_10242dc1_1:
msgid "[_miranda_] ..."
