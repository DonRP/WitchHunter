# game/scripts/quests/newspaperSection/newspaper_section_reading_evening.rpy:220 #-#-# translate crowdin newspaper_section_reading_evening_96b11b1a:
msgid "[_martin_] Кажется теперь моя очередь!"
msgstr "[_martin_] Je pense que c'est mon tour maintenant !"

# game/scripts/quests/newspaperSection/newspaper_section_reading_evening.rpy:224 #-#-# translate crowdin newspaper_section_reading_evening_e416a561:
