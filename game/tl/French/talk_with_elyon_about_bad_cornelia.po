# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:8 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_5059e0a4:
msgid "[_cedrick_] Что стряслось, элион? У тебя хмурый вид."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:11 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_c7d80b81:
msgid "[_elion_] Кажется, я окончательно поругалась с корнелией... Я не знаю, что мне делать.."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:12 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_09ffe8ce:
msgid "[_elion_] Вроде бы лучшая подруга, а проводит все свое время с Вилл и ее подружками.. договорились с ней пойти в кино, а она не пришла..."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:15 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_dcf73abd:
msgid "[_cedrick_] так бывает... со временем многие люди, которые казались нам друзьями — предают нас, но не стоит переживать из-за этого... ты еще найдешь себе новых.."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:18 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_56d8bdcb:
msgid "[_elion_] Но я не хочу новых, меня устраивали мои старые друзья.. а теперь Брайан с Алхими исчезли... теперь еще и это..."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:21 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_fa32c074:
msgid "[_elion_] Хорошо, что у меня есть ты, а то бы я даже и не знала с кем поговорить об этом.."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:22 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_c56e45d9:
msgid "[_cedrick_] ты всегда можешь поговорить со мной, элион. о чем угодно..."
msgstr ""

# game/scripts/quests/getCloseToElyon/talk_with_elyon_about_bad_cornelia.rpy:25 #-#-# translate crowdin talk_with_elyon_about_bad_cornelia_cd201c19:
msgid "[_elion_] Спасибо, Рик... ты очень добр ко мне..."
msgstr ""

