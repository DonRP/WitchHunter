# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:7 #-#-# translate crowdin get_crystal_essence_for_will_825983c1:
msgid "[_cedrick_] Хм, так, давай-ка еще раз. То есть, ты говоришь, что на стражницах зелье не сработает?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:11 #-#-# translate crowdin get_crystal_essence_for_will_c3b3df82:
msgid "[_miranda_] Это ты так это воспринял.. Я сказала, что им не снесет крышу так же сильно, как Алхими"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:15 #-#-# translate crowdin get_crystal_essence_for_will_1da91291:
msgid "[_cedrick_] ...Но почему?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:19 #-#-# translate crowdin get_crystal_essence_for_will_9cf020f5:
msgid "[_miranda_] Ох, бедняжка, вижу ты сильно разочарован"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:23 #-#-# translate crowdin get_crystal_essence_for_will_49316299:
msgid "[_cedrick_] ..."
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:27 #-#-# translate crowdin get_crystal_essence_for_will_1556aafb:
msgid "[_miranda_] Дело в том, что Алхими была простым человеком и поэтому любое, даже самое элементарное заклинание, работает со стопроцентной эффективностью"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:31 #-#-# translate crowdin get_crystal_essence_for_will_2241d0b9:
msgid "[_cedrick_] А стражницы значит.."
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:35 #-#-# translate crowdin get_crystal_essence_for_will_e128856a:
msgid "[_miranda_] Они обладают магической устойчивостью, при чем довольно мощной. Чем больше магической силы в любом существе, тем менее эффективно работает на нем заклинание"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:39 #-#-# translate crowdin get_crystal_essence_for_will_5c530fe4:
msgid "[_cedrick_] ..То есть оно не сработает"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:43 #-#-# translate crowdin get_crystal_essence_for_will_8b07c8af:
msgid "[_miranda_] Да нет же.. Невозможно полностью свести магический эффект на нет, так даже Фобос не может, разве что только его сестре это под силу"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:47 #-#-# translate crowdin get_crystal_essence_for_will_318b9946:
msgid "[_cedrick_] Короче.. Заклинание станет слабее, это я понял. Но на сколько?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:51 #-#-# translate crowdin get_crystal_essence_for_will_497918ed:
msgid "[_miranda_] Хммм.. Эффективность убавится процентов на 20-30, примерно"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:55 #-#-# translate crowdin get_crystal_essence_for_will_7d9c27d3:
msgid "[_cedrick_] Охренеть.. А как этот эффект усилить?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:59 #-#-# translate crowdin get_crystal_essence_for_will_501d897c:
msgid "[_miranda_] ..То есть, если стражница не упадет моментально пред тобой на колени и не станет умолять о твоем члене, то тебя это уже не устраивает?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:63 #-#-# translate crowdin get_crystal_essence_for_will_5261a247:
msgid "[_cedrick_] Честно говоря, нет"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:67 #-#-# translate crowdin get_crystal_essence_for_will_eedd2db0:
msgid "[_miranda_] Боже.. Она все так же будет от тебя без ума, просто.. В разумных пределах"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:71 #-#-# translate crowdin get_crystal_essence_for_will_ecff3f1e:
msgid "[_cedrick_] Хм.. Звучит не убедительно"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:75 #-#-# translate crowdin get_crystal_essence_for_will_d417c8cd:
msgid "[_miranda_] Эх.. Ты можешь усилить эффект с помощью дозировок"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:79 #-#-# translate crowdin get_crystal_essence_for_will_925cd7f1:
msgid "[_cedrick_] То есть? А в чем разница, дам я ей зелье разом сейчас или дозами?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:83 #-#-# translate crowdin get_crystal_essence_for_will_55eef204:
msgid "[_miranda_] Наличие магической силы может сыграть с ней злую шутку. Магия работает как иммунитет - она враждебна к одним организмам и предрасположена к другим"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:87 #-#-# translate crowdin get_crystal_essence_for_will_1f42326f:
msgid "[_miranda_] Кристаллическая эссенция королевы морпионов, в этом плане, довольно подлое заклинание, его нельзя интерпретировать как однозначно плохое или однозначно хорошее"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:88 #-#-# translate crowdin get_crystal_essence_for_will_2b8adf6d:
msgid "[_miranda_] Если дать эссенцию разом в полном объеме, то магическая защита отреагирует враждебно на столь жесткое вмешательство"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:92 #-#-# translate crowdin get_crystal_essence_for_will_db96e0f2:
msgid "[_miranda_] Но, если выбрать правильную дозировку, возможно, организм начнет воспринимать эссенцию как лекарство, самостоятельно усиливая эффект внутри организма"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:96 #-#-# translate crowdin get_crystal_essence_for_will_17d13237:
msgid "[_cedrick_] То есть, при правильной дозировке эссенция может сработать на все сто?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:100 #-#-# translate crowdin get_crystal_essence_for_will_81dc3471:
msgid "[_miranda_] Именно"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:104 #-#-# translate crowdin get_crystal_essence_for_will_9ae835e2:
msgid "[_cedrick_] Мда, вода камень точит"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:108 #-#-# translate crowdin get_crystal_essence_for_will_cde30fb8:
msgid "[_miranda_] ..Что?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:112 #-#-# translate crowdin get_crystal_essence_for_will_8697b42d:
msgid "[_cedrick_] Эм, людская поговорка.. Не важно, главное, что это может сработать. Но какая должна быть дозировка?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:116 #-#-# translate crowdin get_crystal_essence_for_will_7a190e58:
msgid "[_miranda_] По моим подсчетам, нужно не больше одной капли за раз. Получается, для достижения полного эффекта и использования всей эссенции, нужно 5 капель"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:120 #-#-# translate crowdin get_crystal_essence_for_will_c77f3da4:
msgid "[_cedrick_] Понял, отлично. Будем пробовать"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:124 #-#-# translate crowdin get_crystal_essence_for_will_46221034:
msgid "[_miranda_] И еще, Седрик. Учитывая, кхм, специфику эссенции, думаю, для закрепления эффекта, поможет положительная стимуляция"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:128 #-#-# translate crowdin get_crystal_essence_for_will_cb56d210:
msgid "[_cedrick_] ..то есть, сначала капать эссенцию, а потом трахать ее для закрепления эффекта? так а зачем мне тогда вообще это зелье, если я все равно должен ее трахать до наступления эффекта"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:132 #-#-# translate crowdin get_crystal_essence_for_will_6ee97fb0:
msgid "[_miranda_] Ох, Седрик, ты что, в людском мире растерял все свои аристократические манеры? не нужно ее трахать, достаточно даже легкой физической стимуляции.."
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:136 #-#-# translate crowdin get_crystal_essence_for_will_311aca12:
msgid "[_miranda_] Например, простого прикосновения.. Зелье сделает остальную работу за тебя и с каждым разом девушка будет все податливее"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:137 #-#-# translate crowdin get_crystal_essence_for_will_2feb36b6:
msgid "[_miranda_] Твои действия также могут сформировать ее предпочтения. Будешь действовать нежно - она привыкнет к нежности"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:141 #-#-# translate crowdin get_crystal_essence_for_will_901bfacb:
msgid "[_miranda_] И наоборот, будешь действовать грубо и ее тело привыкнет к грубости, хочет она того или нет"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:145 #-#-# translate crowdin get_crystal_essence_for_will_cc1decb5:
msgid "[_cedrick_] Ты так охотно раздаешь советы по соблазнению других девушек. Не знал, что тебе такое нравится"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:149 #-#-# translate crowdin get_crystal_essence_for_will_92356a1d:
msgid "[_miranda_] Ты же сам сказал, что это нужно для задания. К тому же, это Стражницы. Чем жестче ты с ними обращаешься, тем лучше, я не против"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:153 #-#-# translate crowdin get_crystal_essence_for_will_463ac529:
msgid "[_cedrick_] Хах, а мы с тобой, оказывается, на одной волне"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:157 #-#-# translate crowdin get_crystal_essence_for_will_e9e66f62:
msgid "[_miranda_] А чего ты еще ожидал? Мы же спим друг с другом уже несколько сотен лет и до сих пор не сожрали друг друга в процессе. Конечно же это куда больше, чем просто секс"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:161 #-#-# translate crowdin get_crystal_essence_for_will_ce22c4f2:
msgid "[_cedrick_] (Я начинаю понимать, почему я никогда не пересекался с теми, кто еще спал с Мирандой. Видимо жили они после этого не долго. Что ж, видимо я и вправду ей не безразличен)"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:162 #-#-# translate crowdin get_crystal_essence_for_will_24a3b351:
msgid "[_cedrick_] (С другой стороны, за все годы, Миранда тоже единственная девушка, с которой я регулярно спал и которая не закончила свои дни в качестве рабыни в подземелье. Может, я и сам к ней что-то чувствую..)"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:163 #-#-# translate crowdin get_crystal_essence_for_will_ff796c7c:
msgid "[_cedrick_] (Хотя, еще не вечер. Уверен, из нее вышла бы отличная рабыня..)"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:164 #-#-# translate crowdin get_crystal_essence_for_will_502115ab:
msgid "[_Somka108_] ПРОГОЛОСОВАТЬ ЗА ТО, КЕМ В ИТОГЕ СТАНЕТ МИРАНДА ДЛЯ СЕДРИКА ЛЮБОВНИЦЕЙ ИЛИ РАБЫНЕЙ, МОЖНО НА НАШЕМ {a=https://www.patreon.com/posts/63521945}PATREON{/a}"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:168 #-#-# translate crowdin get_crystal_essence_for_will_8485b144:
msgid "[_miranda_] Седрик?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:172 #-#-# translate crowdin get_crystal_essence_for_will_54efcd42:
msgid "[_cedrick_] А, да. Задумался на секунду о своем.. Что ж, спасибо за помощь, Миранда. Я пойду"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:179 #-#-# translate crowdin get_crystal_essence_for_will_e2d4bbde:
msgid "[_miranda_] Удачи там. Будь осторожен.."
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:183 #-#-# translate crowdin get_crystal_essence_for_will_12786eb3:
msgid "[_miranda_] Кстати, а ты уже выбрал стражницу, на которой используешь эссенцию?"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:187 #-#-# translate crowdin get_crystal_essence_for_will_3a9443b7:
msgid "[_cedrick_] Конечно. За все время, только одна стражница до сих пор не попалась мне на крючок, главная среди них.. Вилл"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:192 #-#-# translate crowdin get_crystal_essence_for_will_f77b1ec4:
msgid "[_miranda_] (..Бедная девочка, еще не знает, что ее ждет)"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:199 #-#-# translate crowdin get_crystal_essence_for_will_db3c4f83:
msgid "[_miranda_] Кристаллическая эссенция еще не готова"
msgstr ""

# game/scripts/quests/willCrystalEssence/get_crystal_essence_for_will.rpy:200 #-#-# translate crowdin get_crystal_essence_for_will_894991ec:
msgid "[_cedrick_sprite_] (В таком случае, думаю стоит заняться Элион, пока Миранда делает кристаллическую эссенцию)"
msgstr ""

