# game/scripts/quests/growingUpLilian/kidnapLilianToMeridianCastle.rpy:153 #-#-# translate crowdin kidnapLilianToMeridianCastle_95fa08e9:
msgid "[_cedrick_] Это что!? Твои родители вернулись!?"
msgstr "[_cedrick_] E' così? I tuoi genitori sono tornati!?"

# game/scripts/quests/growingUpLilian/kidnapLilianToMeridianCastle.rpy:155 #-#-# translate crowdin kidnapLilianToMeridianCastle_f6c2bfb4:
msgid "[_cornelia_] Нет, это не они... Они не должны..."
msgstr "[_cornelia_] No, non lo sono... Non dovrebbero..."

