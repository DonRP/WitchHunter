# TODO: Translation updated at 2022-12-22 01:40 #|#|# # §translate crowdin strings: #|#|# # game/scripts/core/quests/QuestTarani.rpy:3
msgid "Тарани Кук"
msgstr "Cuoco Tarani"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Провести первый урок"
msgstr "Tenere la prima lezione"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Провести урок и проверить контрольную работу занизив оценку Тарани"
msgstr "Svolgere la lezione e controllare il lavoro di prova abbassando il voto di Tarani."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Раздать контрольные работы на следующем уроке"
msgstr "Consegnare i test della prossima lezione"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Занизить оценку домашней работы Тарани"
msgstr "Abbassare il voto dei compiti di Tarani"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Поговорить с Тарани после урока"
msgstr "Parlare con Tarani dopo la lezione"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Провести очередной урок"
msgstr "Tenere un'altra lezione"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Распросить о Тарани в школьном клубе журналистике"
msgstr "Chiedere di Tarani al club di giornalismo della scuola"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Достать микрофон для клуба журналистики"
msgstr "Ottenere un microfono per il club di giornalismo"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Отнести микрофон Бэсс и Кортни и разузнать о Тарани"
msgstr "Porta il microfono a Bess e Courtney e scopri la storia di Tarani."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Бланка сделать фотографии Найджела"
msgstr "Chiedete a Blank di fotografare Nigel"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Достать фотоаппарат для Бланка"
msgstr "Ottenere una fotocamera per Blank"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Узнать у миранды где достать Ноздрю дракона"
msgstr "Chiedete a Miranda dove trovare la narice di drago."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Добыть клык Ларвека Миранде в обмен на Ноздрю дракона"
msgstr "Ottenere una zanna Larvek per Miranda in cambio della narice di drago."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Отдать Бланку Ноздрю дракона и фотоаппарат"
msgstr "Dare a Blanca la narice di drago e la macchina fotografica"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Дождаться когда Бланк сделает фотографии Найджела и забрать их"
msgstr "Attendiamo che Blank scatti le foto di Nigel e le ritiri."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Шантажировать Тарани фотографиями Найджела"
msgstr "Ricattare Tarani con foto di Nigel"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Тарани сделать нам приятное после урока"
msgstr "Chiedere a Tarani di farci sentire bene dopo la lezione"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Тарани поработать ртом"
msgstr "Chiedere a Tarani di lavorare con la bocca"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Навестить Тарани у нее дома"
msgstr "Visita a Tarani a casa sua"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Попросить Бланка сделать новые фотографии Найджела"
msgstr "Chiedere a Blank di scattare nuove foto di Nigel"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Добыть Бланку золотую цепочку либо найти ему девушку для секса"
msgstr "Procurare a Blanca una catenina d'oro o trovargli una ragazza con cui fare sesso"

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Навестить Тарани у нее дома вечером и показать ей фотографии Найджела"
msgstr "Stasera andate a trovare Tarani a casa sua e mostratele le foto di Nigel."

# game/scripts/core/quests/QuestTarani.rpy:41
msgid "Поговорить с Тарани в школе"
msgstr "Parlare con Tarani a scuola"

