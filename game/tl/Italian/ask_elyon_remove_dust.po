# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:4 #-#-# translate crowdin ask_elyon_remove_dust_0268f40c:
msgid "[_cedrick_] Элион, можешь подойти на Минутку?"
msgstr "[_cedrick_] Elyon, puoi venire al Minuto?"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:7 #-#-# translate crowdin ask_elyon_remove_dust_b77ac8ba:
msgid "[_elion_] Да, что такое?"
msgstr "[_elion_] Sì, che cos'è?"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:11 #-#-# translate crowdin ask_elyon_remove_dust_76b2ff3a:
msgid "[_cedrick_] Знаешь, мне периодически нужна будет твоя помощь, чтобы убрать пыль с верхних полок. Многие из наших книг – редкие и очень старые экземпляры. Пыль может испортить их качество"
msgstr "[_cedrick_] Sai, di tanto in tanto avrò bisogno del tuo aiuto per spolverare gli scaffali più alti. Molti dei nostri libri sono copie rare e molto antiche. La polvere può rovinare la loro qualità"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:15 #-#-# translate crowdin ask_elyon_remove_dust_8f430b98:
msgid "[_cedrick_] Я бы и сам взялся за эту работу, но кто-то должен всегда стоять за прилавком"
msgstr "[_cedrick_] Io stesso accetterei il lavoro, ma c'è sempre qualcuno che deve stare dietro al bancone."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:18 #-#-# translate crowdin ask_elyon_remove_dust_884abba4:
msgid "[_elion_] Да, конечно, без проблем, Рик. Только схожу за тряпкой и займусь этим!"
msgstr "[_elion_] Sì, certo, nessun problema, Rick. Vado a prendere uno straccio e vado avanti!"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:21 #-#-# translate crowdin ask_elyon_remove_dust_a20cefa7:
msgid "..."
msgstr "..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:24 #-#-# translate crowdin ask_elyon_remove_dust_19b2de5c:
msgid "*звук телефона*"
msgstr "*(telefono che squilla)"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:25 #-#-# translate crowdin ask_elyon_remove_dust_e62b0a9a:
msgid "[_cedrick_] (Элион что, оставила свой телефон у прилавка?)"
msgstr "[_cedrick_] (Elion ha lasciato il telefono al bancone?)."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:28 #-#-# translate crowdin ask_elyon_remove_dust_674e6647:
msgid "[_cedrick_] (Сообщение от Брайана?)"
msgstr "[_cedrick_] (Un messaggio di Brian?)"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:29 #-#-# translate crowdin ask_elyon_remove_dust_844fba11:
msgid "[_cedrick_] (Хм.. Это шанс для меня, наконец избавиться от этого мелкого паршивца)"
msgstr "[_cedrick_] (Hmm... Questa è l'occasione per sbarazzarmi finalmente di questo piccolo insetto)."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:30 #-#-# translate crowdin ask_elyon_remove_dust_009acf6b:
msgid "[_cedrick_] (Быстро.. Надо скопировать его номер, пока Элион не вернулась)"
msgstr "[_cedrick_] (Veloce... devo copiare il suo numero prima che torni Elyon)"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:34 #-#-# translate crowdin ask_elyon_remove_dust_a20cefa7_1:
msgid "..."
msgstr "..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:40 #-#-# translate crowdin ask_elyon_remove_dust_a20cefa7_2:
msgid "..."
msgstr "..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:43 #-#-# translate crowdin ask_elyon_remove_dust_ac459c77:
msgid "[_cedrick_] (Хм, может в телефоне есть еще что-то? Адрес его дома, например. Надо поискать в сообщениях..)"
msgstr "[_cedrick_] (Hmm, forse c'è dell'altro sul telefono? L'indirizzo della sua casa, ad esempio. Dovrò cercarlo nei miei messaggi...)"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:47 #-#-# translate crowdin ask_elyon_remove_dust_b5a03662:
msgid "[_cedrick_] (Хах, ну надо же. Это кое что получше адреса Брайана. Не ожидал такого от нашей скромницы Элион...)"
msgstr "[_cedrick_] (Huh, wow. È qualcosa di meglio dell'indirizzo di Brian. Non me lo aspettavo dal nostro umile Alion...)."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:48 #-#-# translate crowdin ask_elyon_remove_dust_a20cefa7_3:
msgid "..."
msgstr "..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:49 #-#-# translate crowdin ask_elyon_remove_dust_a20cefa7_4:
msgid "..."
msgstr "..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:50 #-#-# translate crowdin ask_elyon_remove_dust_c00ce005:
msgid "[_cedrick_] (Черт, она почти закончила.. Нет времени дальше копаться в телефоне...)"
msgstr "[_cedrick_] (Dannazione, ha quasi finito... Non c'è tempo per scavare ulteriormente nel telefono...)"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:55 #-#-# translate crowdin ask_elyon_remove_dust_8eb33cb9:
msgid "[_elion_] Я закончила! Фух, это было сложнее, чем я думала.."
msgstr "[_elion_] Ho finito! Uff, è stato più difficile di quanto pensassi..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:56 #-#-# translate crowdin ask_elyon_remove_dust_5835920e:
msgid "[_cedrick_] А, м... Отличная работа, Элион.. Ты молодец!"
msgstr "[_cedrick_] Ah, ehm... Ben fatto, Elyon... Hai fatto un ottimo lavoro!"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:59 #-#-# translate crowdin ask_elyon_remove_dust_48b910b9:
msgid "[_elion_] Ага, спасибо, Рик"
msgstr "[_elion_] Sì, grazie, Rick."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:62 #-#-# translate crowdin ask_elyon_remove_dust_6fd08802:
msgid "[_elion_] О, вот и мой телефон, а я его искала"
msgstr "[_elion_] Oh, ecco il mio telefono, e lo stavo cercando..."

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:63 #-#-# translate crowdin ask_elyon_remove_dust_6a7f3c9e:
msgid "[_cedrick_] (Фух, чуть не попался...)"
msgstr "[_cedrick_] (Uff, per poco non mi beccano...)"

# game/scripts/quests/dustInBookshop/ask_elyon_remove_dust.rpy:64 #-#-# translate crowdin ask_elyon_remove_dust_942bf6ee:
msgid "[_cedrick_] (Нужно будет написать этому мелкому паршивцу и разобраться с ним прежде чем он не стал большей помехой)"
msgstr "[_cedrick_] (Dovremo scrivere a quel piccolo bastardo e occuparci di lui prima che diventi un fastidio maggiore)."

