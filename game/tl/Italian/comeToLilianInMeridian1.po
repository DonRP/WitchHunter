# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:9 #-#-# translate crowdin comeToLilianInMeridian1_8fb40efe:
msgid "[_lilian_] Рик, я так рада тебя видеть!"
msgstr "[_lilian_] Rick, è così bello vederti!"

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:10 #-#-# translate crowdin comeToLilianInMeridian1_e8e3cedc:
msgid "[_cedrick_] А у меня для тебя есть сюрприз..."
msgstr "[_cedrick_] E ho una sorpresa per voi..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:14 #-#-# translate crowdin comeToLilianInMeridian1_d553e6c2:
msgid "[_lilian_] Сюрприз!? Я просто обожаю сюрпризы, Рик!"
msgstr "[_lilian_] Sorpresa!? Adoro le sorprese, Rick!"

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:18 #-#-# translate crowdin comeToLilianInMeridian1_17d4250b:
msgid "[_cedrick_] У меня есть то, что сделает тебя взрослой! И ты станешь такой же, как и твоя сестра..."
msgstr "[_cedrick_] Ho qualcosa che vi farà diventare adulti! E sarai proprio come tua sorella..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:22 #-#-# translate crowdin comeToLilianInMeridian1_0e9590f2:
msgid "[_cedrick_] Это волшебный лимонад..."
msgstr "[_cedrick_] È una limonata magica..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:26 #-#-# translate crowdin comeToLilianInMeridian1_00c2359f:
msgid "[_lilian_] Волшебный... лимонад?"
msgstr "[_lilian_] Magia... limonata?"

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:27 #-#-# translate crowdin comeToLilianInMeridian1_3406caea:
msgid "[_cedrick_] Да, вроде того. Пей, не бойся..."
msgstr "[_cedrick_] Sì, più o meno. Bevete, non abbiate paura..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:31 #-#-# translate crowdin comeToLilianInMeridian1_eef3d7eb:
msgid "[_lilian_] Ну... Раз ты так говоришь... Я его выпью, я очень хочу стать взрослой..."
msgstr "[_lilian_] Beh... Se lo dici tu... Lo berrò, voglio davvero essere un adulto..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:34 #-#-# translate crowdin comeToLilianInMeridian1_a20cefa7:
msgid "..."
msgstr "..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:35 #-#-# translate crowdin comeToLilianInMeridian1_527bb860:
msgid "[_lilian_] Что-то не похоже на лимонад..."
msgstr "[_lilian_] Non sembra una limonata..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:36 #-#-# translate crowdin comeToLilianInMeridian1_d72cf41d:
msgid "[_cedrick_]  Тебе придется выпить его до дна, иначе волшебство не произойдет."
msgstr "[_cedrick_]  Bisogna bere, altrimenti la magia non avverrà."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:37 #-#-# translate crowdin comeToLilianInMeridian1_a20cefa7_1:
msgid "..."
msgstr "..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:38 #-#-# translate crowdin comeToLilianInMeridian1_6dceb911:
msgid "[_cedrick_] Ну... Вот и славно..."
msgstr "[_cedrick_] Beh... Bene..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:39 #-#-# translate crowdin comeToLilianInMeridian1_74ee5d42:
msgid "[_cedrick_] Приляг, отдохни... Зелье должно сработать через некоторое время..."
msgstr "[_cedrick_] Sdraiati, riposa un po'... La pozione dovrebbe funzionare dopo un po'..."

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:40 #-#-# translate crowdin comeToLilianInMeridian1_be548837:
msgid "[_cedrick_] Я скоро вернусь!"
msgstr "[_cedrick_] Tornerò presto!"

# game/scripts/quests/growingUpLilian/comeToLilianInMeridian/comeToLilianInMeridian1.rpy:41 #-#-# translate crowdin comeToLilianInMeridian1_a20cefa7_2:
msgid "..."
msgstr "..."

