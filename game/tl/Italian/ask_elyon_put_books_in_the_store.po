# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:4 #-#-# translate crowdin ask_elyon_put_books_in_the_store_42ca9224:
msgid "[_cedrick_] Элион, помоги мне с кое-чем..."
msgstr "[_cedrick_] Elyon, aiutami con una cosa..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:7 #-#-# translate crowdin ask_elyon_put_books_in_the_store_732b1932:
msgid "[_elion_] Конечно! С чем именно?"
msgstr "[_elion_] Naturalmente! Con cosa esattamente?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:10 #-#-# translate crowdin ask_elyon_put_books_in_the_store_0f91d53f:
msgid "[_cedrick_] Думаю, сегодня нужно наконец-таки выставить пылящиеся в коробках книги..."
msgstr "[_cedrick_] Penso che i libri che hanno preso polvere nelle loro scatole dovrebbero finalmente essere messi fuori oggi..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:11 #-#-# translate crowdin ask_elyon_put_books_in_the_store_bed04061:
msgid "[_cedrick_] Насколько я понимаю, там все перепутано и выставлять нужно на полку с соответствующим жанром и по алфавитному порядку начиная от фамилии автора..."
msgstr "[_cedrick_] Per quanto ne so, è tutto mischiato e dovrebbe essere accantonato con il genere appropriato e in ordine alfabetico a partire dal cognome dell'autore..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:14 #-#-# translate crowdin ask_elyon_put_books_in_the_store_779750e6:
msgid "[_elion_] Хорошо, звучит не очень сложно..."
msgstr "[_elion_] Ok, non sembra molto complicato..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:17 #-#-# translate crowdin ask_elyon_put_books_in_the_store_566befa9:
msgid "[_cedrick_] Это ты еще не видела сколько там коробок..."
msgstr "[_cedrick_] Non hai visto quante scatole ci sono..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:19 #-#-# translate crowdin ask_elyon_put_books_in_the_store_8dabefc6:
msgid ".."
msgstr ".."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:20 #-#-# translate crowdin ask_elyon_put_books_in_the_store_5f9b772e:
msgid "...."
msgstr "...."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:21 #-#-# translate crowdin ask_elyon_put_books_in_the_store_57e69339:
msgid "[_elion_] Ого... Знаешь, я не совсем уверена, что мы справимся за день!"
msgstr "[_elion_] Wow... Sai, non sono sicuro che riusciremo a farlo in un giorno!"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:22 #-#-# translate crowdin ask_elyon_put_books_in_the_store_17355864:
msgid "[_cedrick_] Ну необязательно делать все сразу. Мы вполне можем растянуть это и на несколько недель..."
msgstr "[_cedrick_] Non dobbiamo fare tutto in una volta. Possiamo estenderlo su più settimane..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:23 #-#-# translate crowdin ask_elyon_put_books_in_the_store_fe1c8bb2:
msgid "[_elion_] Ну, в таком случае, приступим!"
msgstr "[_elion_] In questo caso, andiamo avanti!"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:24 #-#-# translate crowdin ask_elyon_put_books_in_the_store_6fdcc24f:
msgid "[_centered_] {size=36}Cпустя пол часа{/size}"
msgstr "[_centered_] {Mezz'ora più tardi."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:26 #-#-# translate crowdin ask_elyon_put_books_in_the_store_08487bdc:
msgid "[_cedrick_] (Фуф... Что-то я устал... какая утомительная работа...)"
msgstr "[_cedrick_] (Uff... sono stanco... che lavoro faticoso...)"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:27 #-#-# translate crowdin ask_elyon_put_books_in_the_store_c244a924:
msgid "[_cedrick_] (Интересно, как там Элион...)"
msgstr "[_cedrick_] (Mi chiedo come stia Elyon...)."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:28 #-#-# translate crowdin ask_elyon_put_books_in_the_store_a20cefa7:
msgid "..."
msgstr "..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:32 #-#-# translate crowdin ask_elyon_put_books_in_the_store_7802be40:
msgid "[_elion_]  Ну вот я и закончила с очередной коробкой!"
msgstr "[_elion_]  Eccomi qui, finito con un'altra scatola!"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:36 #-#-# translate crowdin ask_elyon_put_books_in_the_store_04fc0a65:
msgid "[_cedrick_sprite_] А ты молодец! Смотрю, ты уже довольно много сделала..."
msgstr "[_cedrick_sprite_] Ben fatto! Vedo che hai già fatto molto..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:39 #-#-# translate crowdin ask_elyon_put_books_in_the_store_af45d626:
msgid "[_elion_] Фуф, ага... Думаю, разберу еще одну коробку и хватит на сегодня... Так неудобно работать в юбке..."
msgstr "[_elion_] Ah, sì... Penso che disimballerò un'altra scatola e la chiuderò qui... È così scomodo lavorare in gonna..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:40 #-#-# translate crowdin ask_elyon_put_books_in_the_store_3af0d11d:
msgid "[_cedrick_] Действительно..."
msgstr "[_cedrick_] Infatti..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:42 #-#-# translate crowdin ask_elyon_put_books_in_the_store_a20cefa7_1:
msgid "..."
msgstr "..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:46 #-#-# translate crowdin ask_elyon_put_books_in_the_store_a20cefa7_2:
msgid "..."
msgstr "..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:50 #-#-# translate crowdin ask_elyon_put_books_in_the_store_02136114:
msgid "[_elion_] Милые кости?"
msgstr "[_elion_] Ossa carine?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:53 #-#-# translate crowdin ask_elyon_put_books_in_the_store_56aae87d:
msgid "[_elion_] Это же моя любимая книга!"
msgstr "[_elion_] È il mio libro preferito!"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:57 #-#-# translate crowdin ask_elyon_put_books_in_the_store_88883a36:
msgid "[_cedrick_] Звучит жутковато... Не знал, что тебе такое нравится..."
msgstr "[_cedrick_] Sembra inquietante... Non sapevo che ti piacesse questo genere di cose..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:58 #-#-# translate crowdin ask_elyon_put_books_in_the_store_6d77b9a8:
msgid "[_cedrick_]  (видать сказываются гены некоторых членов семейства Эсканор, хе-хе...)"
msgstr "[_cedrick_]  (Immagino che i geni di alcuni membri della famiglia Escanor entrino in gioco, hehehe...)"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:61 #-#-# translate crowdin ask_elyon_put_books_in_the_store_24597d3e:
msgid "[_elion_] Нет, вовсе нет! Разве ты ее не читал?"
msgstr "[_elion_] No, per niente! Non l'hai letto?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:64 #-#-# translate crowdin ask_elyon_put_books_in_the_store_52469ef2:
msgid "[_cedrick_] Скорей всего нет, а о чем книга?"
msgstr "[_cedrick_] Probabilmente no, ma di cosa parla il libro?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:67 #-#-# translate crowdin ask_elyon_put_books_in_the_store_ac0ad4db:
msgid "[_elion_] Об убитой четырнадцатилетней девушке... Это если не вдаваться в подробности..."
msgstr "[_elion_] Su una quattordicenne assassinata... Questo se non si entra nei dettagli..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:68 #-#-# translate crowdin ask_elyon_put_books_in_the_store_4729d02d:
msgid "[_elion_] После своей смерти она с небес наблюдает за тем, как ее родные отреагировали на ее смерть и пытаются жить дальше..."
msgstr "[_elion_] Dopo la sua morte, osserva dal cielo la reazione della sua famiglia alla sua morte e cerca di andare avanti con le loro vite..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:69 #-#-# translate crowdin ask_elyon_put_books_in_the_store_3a53c2d7:
msgid "[_elion_] наверное, если бы я исчезла как она, никто бы толком и не заметил..."
msgstr "[_elion_] Credo che se sparissi come lei, nessuno se ne accorgerebbe..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:70 #-#-# translate crowdin ask_elyon_put_books_in_the_store_f6460587:
msgid "[_cedrick_] Твои друзья бы точно заметили."
msgstr "[_cedrick_] I vostri amici lo noteranno sicuramente."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:71 #-#-# translate crowdin ask_elyon_put_books_in_the_store_da4e32a6:
msgid "[_elion_] У меня и друзей то нет..."
msgstr "[_elion_] Non ho nemmeno amici..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:72 #-#-# translate crowdin ask_elyon_put_books_in_the_store_544b11d4:
msgid "[_cedrick_] А как же корнелия?"
msgstr "[_cedrick_] E Cornelia?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:73 #-#-# translate crowdin ask_elyon_put_books_in_the_store_5e169d37:
msgid "[_elion_] У нас с ней... такие себе отношения"
msgstr "[_elion_] Io e lei abbiamo... questo tipo di relazione."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:74 #-#-# translate crowdin ask_elyon_put_books_in_the_store_94abb374:
msgid "[_elion_] Да и с родителям тоже что-то не так... иногда мне кажется, что им нет до меня никакого дела..."
msgstr "[_elion_] C'è qualcosa che non va anche nei miei genitori... A volte mi sembra che a loro non importi nulla di me..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:75 #-#-# translate crowdin ask_elyon_put_books_in_the_store_a5c0c846:
msgid "[_cedrick_] Ну, в таком случае я бы точно заметил твою пропажу..."
msgstr "[_cedrick_] Beh, in questo caso, mi sarei accorto della tua assenza..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:76 #-#-# translate crowdin ask_elyon_put_books_in_the_store_89c59728:
msgid "[_elion_] ..."
msgstr "[_elion_] ..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:79 #-#-# translate crowdin ask_elyon_put_books_in_the_store_686ccda3:
msgid "[_elion_] Ты правда целовался с корнелией?"
msgstr "[_elion_] Hai davvero baciato Cornelia?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:80 #-#-# translate crowdin ask_elyon_put_books_in_the_store_42c67298:
msgid "[_cedrick_] Это была она... просто случайность..."
msgstr "[_cedrick_] Era lei... solo un incidente..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:81 #-#-# translate crowdin ask_elyon_put_books_in_the_store_d5d3d6e0:
msgid "[_elion_] И вы теперь... встречаетесь?"
msgstr "[_elion_] E ora sei... appuntamenti?"

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:82 #-#-# translate crowdin ask_elyon_put_books_in_the_store_09549f5c:
msgid "[_cedrick_] Эм... вовсе нет... мы просто друзья..."
msgstr "[_cedrick_] Ehm... per niente... siamo solo amici..."

# game/scripts/quests/boxWithBooks/ask_elyon_put_books_in_the_store.rpy:84 #-#-# translate crowdin ask_elyon_put_books_in_the_store_a20cefa7_3:
msgid "..."
msgstr "..."

