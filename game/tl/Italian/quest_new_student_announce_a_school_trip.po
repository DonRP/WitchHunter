# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:28 #-#-# translate crowdin quest_new_student_announce_a_school_trip_db883e25:
msgid "[_matt_] Да, я поеду.."
msgstr "[_matt_] Sì, ci andrò..."

# game/scripts/quests/newStudent/quest_new_student_announce_a_school_trip.rpy:29 #-#-# translate crowdin quest_new_student_announce_a_school_trip_98561895:
