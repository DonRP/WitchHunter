# TODO: Translation updated at 2022-12-22 01:40 #|#|# # game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:6 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_d6feab25:
msgid "[_haylin_] Эй, Рик! Пришел помочь?"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:10 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_c53dbfd1:
msgid "[_cedrick_] Да, вроде того, что мне нужно делать?"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:14 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_526f0001:
msgid "[_haylin_]  Эм... Ты умеешь готовить?"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:15 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_1a4876e5:
msgid "[_cedrick_] Ну, если честно, то не особо…"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:19 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_05d0a005:
msgid "[_haylin_] Ну ладно, ничего страшного. Можешь поработать официантом? Просто берешь заказы у посетителей и идешь с ними на кухню..."
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:20 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_1bfa61b9:
msgid "[_cedrick_] Думаю, что я смогу с этим справиться..."
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:24 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_628c343c:
msgid "[_haylin_] Я так тебе благодарна!"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:25 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_0b883b7c:
msgid "[_cedrick_] Не стоит. Что ж... Я могу приступать, босс?"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:29 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_205a57f0:
msgid "[_haylin_] Да, конечно!"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:33 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_f3af1b6c:
msgid "[_cedrick_] Мне, вроде, нужна какая-то форма или вроде того?"
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:38 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_69b03436:
msgid "[_haylin_] Нет, ты и так выглядишь довольно деловито..."
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:43 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_ccbf14dd:
msgid "[_haylin_] Ну а меню, я думаю, ты уже успел изучить. Можешь советовать посетителям те блюда, которые тебе больше понравилось..."
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:47 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_6a648b41:
msgid "[_cedrick_] Да, вроде того..."
msgstr ""

# game/scripts/quests/haylin/ql_hayLin_help_in_restoran_1.rpy:50 #-#-# translate crowdin ql_hayLin_help_in_restoran_1_f7f35501:
msgid "Седрик работает в ресторане Серебряный дракон. Не все идет так гладко, как хотелось бы, но Хай Лин благодарна за вашу помощь…"
msgstr ""

